<odoo>
    <record id="hex_hex_view_tree" model="ir.ui.view">
        <field name="name">hex.hex.tree</field>
        <field name="model">hex.hex</field>
        <field name="arch" type="xml">
            <tree edit="0" delete="0" create="0">
                <field name="name"/>
                <field name="code"/>
                <field name="index" readonly="1"/>
                <field name="circle_order"/>
                <field name="circle_number"/>
                <field name="color" readonly="1"/>
                <field name="quad_id"/>
            </tree>
        </field>
    </record>

    <record id="hex_hex_view_form" model="ir.ui.view">
        <field name="name">hex.hex.form</field>
        <field name="model">hex.hex</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <group>
                            <!-- <div class="hex hex_hex_form"/>-->
                            <field name="tmp_str1" widget="code"/>
                            <field name="tmp_str2" widget="code"/>
                            <!-- <field name="tmp_flt" widget="float_full_time" options="{'round_off': True, 'readonly': True}"/>-->
                        </group>
                        <group>
                            <field name="name"/>
                            <field name="code"/>
                            <field name="index" readonly="1"/>
                            <field name="circle_order"/>
                            <field name="circle_number"/>
                            <field name="color" readonly="1"/>
                            <field name="quad_id" readonly="1"/>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>